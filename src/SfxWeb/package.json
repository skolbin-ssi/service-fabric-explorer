{
  "name": "sfx-web",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config=proxy.conf.json --port 3000",
    "build": "ng build",
    "build:prod": "ng build --prod  --output-hashing=none",
    "build:accesstesting": "ng build --prod --outputPath=../Sfx-Proxy/wwwroot --configuration=accesstesting --output-hashing=none",
    "test": "ng test",
    "cypress:local": "cypress open --env API_PREFIX=api",
    "ci:start-server": "angular-http-server --path ./dist -p 3000 --silent",
    "ci:test-build": "ng build --configuration=accesstesting",
    "ci:test": "ng test --watch=false --browsers=ChromeHeadless",
    "ci:cy-run": "start-server-and-test ci:start-server http://localhost:3000 cy:run",
    "cy:run": "cypress run ",
    "lint": "ng lint",
    "build:stats": "ng build --stats-json",
    "analyze": "webpack-bundle-analyzer dist/stats.json",
    "replaceEnv": "node replaceEnvVars.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.0.2",
    "@angular/cdk": "~11.0.0",
    "@angular/common": "~11.0.2",
    "@angular/compiler": "~11.0.2",
    "@angular/core": "~11.0.2",
    "@angular/forms": "~11.0.2",
    "@angular/localize": "^11.0.2",
    "@angular/material": "^11.0.0",
    "@angular/platform-browser": "~11.0.2",
    "@angular/platform-browser-dynamic": "~11.0.2",
    "@angular/router": "~11.0.2",
    "@microsoft/applicationinsights-web": "^2.5.10",
    "@ng-bootstrap/ng-bootstrap": "^9.0.2",
    "adal-angular": "^1.0.17",
    "bootstrap": "^4.4.1",
    "dayjs": "^1.8.20",
    "highcharts": "^8.1.1",
    "lodash": "^4.17.19",
    "luxon": "^1.22.0",
    "metro4": "^4.3.5",
    "moment": "^2.29.1",
    "nouislider": "^14.6.0",
    "rxjs": "~6.5.4",
    "tslib": "^2.0.0",
    "vis-data": "^6.4.1",
    "vis-timeline": "^7.1.3",
    "vis-uuid": "^1.1.4",
    "xml2js": "^0.4.22",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1100.2",
    "@angular/cli": "~11.0.2",
    "@angular/compiler-cli": "~11.0.2",
    "@angular/language-service": "~11.0.2",
    "@types/adal": "^1.0.29",
    "@types/d3": "^5.7.2",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "@types/nouislider": "^9.0.6",
    "angular-http-server": "^1.9.0",
    "codelyzer": "^6.0.0",
    "cypress": "^7.1.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.1.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "start-server-and-test": "^1.12.0",
    "ts-node": "~7.0.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.5",
    "webpack-bundle-analyzer": "^3.6.0"
  }
}
